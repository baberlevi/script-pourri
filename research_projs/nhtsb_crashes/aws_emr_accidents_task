CREATE EXTERNAL TABLE IF NOT EXISTS accident (
STATE string,
ST_CASE string,
VE_TOTAL string,
VE_FORMS string,
PVH_INVL string,
PEDS string,
PERNOTMVIT string,
PERMVIT string,
PERSONS string,
COUNTY string,
CITY string,
DAY string,
MONTH string,
YEAR string,
DAY_WEEK string,
HOUR string,
MINUTE string,
NHS string,
RUR_URB string,
FUNC_SYS string,
RD_OWNER string,
ROUTE string,
TWAY_ID string,
TWAY_ID2 string,
MILEPT string,
LATITUDE string,
LONGITUD string,
SP_JUR string,
HARM_EV string,
MAN_COLL string,
RELJCT1 string,
RELJCT2 string,
TYP_INT string,
WRK_ZONE string,
REL_ROAD string,
LGT_COND string,
WEATHER1 string,
WEATHER2 string,
WEATHER string,
SCH_BUS string,
RAIL string,
NOT_HOUR string,
NOT_MIN string,
ARR_HOUR string,
ARR_MIN string,
HOSP_HR string,
HOSP_MN string,
CF1 string,
CF2 string,
CF3 string,
FATALS string,
DRUNK_DR string
)

ROW FORMAT SERDE 'org.apache.hadoop.hive.serde2.OpenCSVSerde'
LOCATION 's3://us-west-2.amazonaws.com/isu-sdsi-2016-nhtsa/input/accident.csv';

SELECT COUNT(*) count FROM accident WHERE state='19';
